{
    "id": "io.gitlab.floodlight.Presenter",
    "runtime": "org.gnome.Platform",
    "runtime-version": "48",
    "sdk": "org.gnome.Sdk",
    "command": "floodlight-presenter",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--socket=pulseaudio",
        "--device=dri",
        "--share=network",
        "--filesystem=host:ro"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/lib/girepository-1.0",
        "/share/gir-1.0",
        "/share/gtk-doc",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "presenter",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "git",
                    "url": "https://gitlab.com/floodlight/presenter.git",
                    "commit": "a462c4ad08f1a3fa2d45a24004cce8da87121b52"
                }
            ]
        }
    ]
}
